snippet     plugin
abbr        plugin
    scriptencoding utf-8
    if exists('g:loaded_`expand("%:t:r")`')
      finish
    endif
    let g:loaded_`expand("%:t:r")` = 1

    let s:save_cpo = &cpo
    set cpo&vim

    ${0}

    let &cpo = s:save_cpo
    unlet s:save_cpo

snippet lambda
alias lam
  {k, v -> ${0}}
